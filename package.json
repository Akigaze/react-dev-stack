    "example.todolist": "webpack-dev-server --open --config example/todo-list/webpack.config.example.todolist.js",
    "jest": "jest --coverage",
    "jest.counter": "jest --config=example/counter/jest.config.js",
    "jest.todolist": "jest --config=example/todo-list/jest.config.js",
    "jasmine": "jasmine --config=__tests__/support/jasmine.json",
    "jasmine.counter": "jasmine --config=example/counter/__tests__/support/jasmine.json",
    "jasmine.todolist": "jasmine --config=example/todo-list/__tests__/support/jasmine.json"
